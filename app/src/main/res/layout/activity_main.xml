            <TextView
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:text="asfdas" />

        </android.support.v7.widget.Toolbar>




    </LinearLayout>
</layout>